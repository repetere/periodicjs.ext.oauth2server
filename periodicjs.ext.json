{
	"periodicCompatibility":"5.0.0",
	"periodicDependencies": [{
      "extname":"periodicjs.ext.asyncadmin",
      "version":"~6.0.0"
    },{
      "extname":"periodicjs.ext.login",
      "version":"~7.0.3"
    }],
	"periodicjs.ext.asyncadmin": {
		"menu": {
			"Extensions": {
				"oauth2server":{
					"userprivilege":"110",
          "tree-item": "folder",
